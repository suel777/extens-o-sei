{
    "manifest_version": 3,
    "name": "Color Sei",
    "version": "1.0",
    "description": "Destaque partes importantes de processos eletr√¥nicos.",
    "icons": {
      "128": "colorsei.png"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "colorsei.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  